function foa_event_node_info() {
  $items = array(
    'foa_event' => array(
      'name' => t('foa_event'),
      'base' => 'node_content',
      'description' => '',
      'has_title' => '1',
      'title_label' => t('Title'),
      'help' => '',
    ),
  );
  return $items;
}
